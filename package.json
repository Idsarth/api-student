{
  "name": "insafe-student",
  "version": "1.0.0",
  "description": "InSafe student api rest",
  "main": "main.ts",
  "scripts": {
    "start": "node ./dist/main.js",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "env:prod": "cross-env NODE_ENV=PRODUCTION node ./dist/mina.js",
    "env:dev": "cross-env NODE_ENV=DEVELOPMENT nodemon --exec \"npm run build && npm run start\" --watch src --ext ts",
    "env:staging": "cross-env NODE_ENV=STANGING nodemon --exec \"npm run build && npm run start\" --watch src --ext ts",
    "test": ""
  },
  "keywords": [
    "insafe",
    "apirest",
    "typescript",
    "docker",
    "express"
  ],
  "author": "Idsarth <idsarthdev19@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.828.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.3.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.11.10",
    "multer": "^1.4.2",
    "socket.io": "^3.0.5",
    "socket.io-redis": "^6.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.20",
    "@types/supertest": "^2.0.10",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "_moduleAliases": {
    "@common/enums": "dist/src/common/enums",
    "@common/enums/*": "dist/src/common/enums/*",

    "@common/exceptions": "dist/src/common/exceptions",
    "@common/exceptions/*": "dist/src/common/exceptions/*",

    "@common/services": "dist/src/common/services",
    "@common/services/*": "dist/src/common/services/*",

    "@common/interfaces": "dist/src/common/interfaces",
    "@common/interfaces/*": "dist/src/common/interfaces/*",

    "@common/middlew": "dist/src/common/middlew",
    "@common/middlew/*": "dist/src/common/middlew/*",

    "@common/types": "dist/src/common/types",
    "@common/types/*": "dist/src/common/types/*",

    "@config": "dist/src/config",
    "@config/*": "dist/src/config/*",

    "@handlers": "dist/src/handlers",
    "@handlers/*": "dist/src/handlers/*",

    "@providers": "dist/src/providers",
    "@providers/*": "dist/src/providers/*",

    "@swagger": "dist/src/swagger",
    "@swagger/*": "dist/src/swagger/*",

    "@modules": "dist/src/modules",
    "@modules/*": "dist/src/modules/*"
  }
}
